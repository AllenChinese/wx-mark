/* pages/me/me.wxss */

.mePage__container {}

/* 用户信息 */

.mePage__userMsg-part {
  height: 250rpx;
  line-height: 250rpx;
  font-size: 28rpx;
}

.mePage_userMsg-content {
  padding: 0 40rpx;
}

.mePage__person-img {
  position: relative;
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  width: 140rpx;
  height: 140rpx;
  vertical-align: middle;
  box-shadow: 0 0 10rpx 2rpx rgba(255, 255, 255, .8);
}

.mePage_msg {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  margin-top: 75rpx;
}

.mePage_msg view {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
}

.mePage_msg .mePage_msg--personal {
  display: flex;
  margin-bottom: 18rpx;
  font-family: 'PingFangSC-Semibold';
  font-size: 36rpx;
  color: #c2c2c2;
}

.mePage_msg .mePage_msg--personal .personal_model {
  margin-left: 10rpx;
  vertical-align: middle;
}

.mePage_msg .mePage_msg--personal .personal_level {
  padding-top: 5rpx;
  font-size: 24rpx;
  font-family: 'PingFangSC-Regular';
}

.mePage_msg .mePage_msg--saying {
  line-height: 24rpx;
  font-family: 'PingFangSC-Regular';
  font-size: 26rpx;
  color: #c2c2c2;
}

.mePage_msg--checkin-col {
  display: flex;
  justify-content: flex-end;
}

.mePage_userMsg-content .mePage_msg--checkin {
  margin: 90rpx 0 0 0;
  border-radius: 30rpx;
  border: 1px solid #ed1941;
  width: 90rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 24rpx;
  -webkit-border-radius: 30rpx;
  -moz-border-radius: 30rpx;
  -ms-border-radius: 30rpx;
  -o-border-radius: 30rpx;
  color: #ed1941;
}

.mePage_userMsg-content .mePage_msg--checkin.active {
  border-color: #c2c2c2;
  color: #c2c2c2;
}

.mePage_userMsg-content .mePage_msg--total {
  margin: 15rpx 0 0 60rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #525252;
}

.mePage__function-part {
  margin: 10rpx auto 50rpx;
  padding-top: 20rpx;
  width: 90%;
  box-shadow: 0 5rpx 30rpx 0 #f2f2f2;
}

.mePage__function-item {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  text-align: center;
}

.mePage__function-part .icon {
  border-radius: 50%;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  background-size: 65% 65%;
  background-position: center center;
  background-repeat: no-repeat;
}

.mePage__function-part .icon__all {
  background-image: url('data:image/png;base64,');
  background-color: #37b9d0;
  background-position: 80% 60%;
}

.mePage__function-part .icon__create {
  background-image: url('data:image/png;base64,');
  background-color: #53a7e5;
}

.mePage__function-part .icon__rank {
  background-image: url('data:image/png;base64,');
  background-color: #5cbee2;
}

.mePage__function-part text {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
}

.second-cell {
  transform: translateY(60rpx);
  -webkit-transform: translateY(60rpx);
  -moz-transform: translateY(60rpx);
  -ms-transform: translateY(60rpx);
  -o-transform: translateY(60rpx);
}

.mePage__conpany-part {
  position: relative;
  margin: 150rpx auto 0;
  width: 220rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 300;
  color: #d2d2d2;
  background-color: #fff;
}

.mePage__conpany-part::before {
  z-index: -1;
  position: absolute;
  content: " ";
  top: 20rpx;
  left: -40rpx;
  width: 300rpx;
  height: 2rpx;
  background-color: #d2d2d2;
}
